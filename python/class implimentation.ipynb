    "custom_obj= ImplementAbs(\"hello\")\n",
    "x=abs(-10)\n",
    "y=abs(-100.254)\n",
    "z=abs(custom_obj)\n",
    "print(x)        \n",
    "print(y)        \n",
    "print(z)        "
